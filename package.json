{
  "name": "@textactor/concept-domain",
  "version": "0.1.2",
  "description": "Core Concept module",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/concept-domain.git"
  },
  "keywords": [
    "domain",
    "concept",
    "textactor"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/concept-domain/issues"
  },
  "homepage": "https://github.com/textactor/concept-domain#readme",
  "devDependencies": {
    "@types/node": "10.12.5",
    "@types/shortid": "^0.0.29",
    "ava": "^0.25.0",
    "rimraf": "^2.6.2",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "ms": "^2.1.1",
    "shortid": "^2.2.14"
  }
}
